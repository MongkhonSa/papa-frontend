{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node --project tsconfig.server.json' server/index.ts",
    "build:server": "tsc --project tsconfig.server.json ",
