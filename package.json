    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@types/express": "^4.17.12",
    "@types/mssql": "^6.0.8",
    "@zeit/next-css": "^1.0.1",
    "antd": "^4.16.2",
    "axios": "^0.21.1",
    "dayjs": "^1.10.5",
    "exceljs": "^4.2.1",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "lint": "^0.7.0",
    "mssql": "^7.1.1",
    "next": "10.2.3",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.2",
    "@types/react": "17.0.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
